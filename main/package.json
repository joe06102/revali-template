{
  "name": "revali-gale",
  "version": "1.0.0",
  "description": "一个支持服务端渲染的react模板",
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src && stylelint \"**/*.css\" \"**/*.less\"",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix src && stylelint \"**/*.css\" \"**/*.less\" --fix",
    "start:dev": "cross-env NODE_ENV=development API_ENV=dev node script/dev.js",
    "start:test": "cross-env NODE_ENV=development API_ENV=test node script/dev.js",
    "start:uat": "cross-env NODE_ENV=development API_ENV=uat node script/dev.js",
    "start:prod": "cross-env NODE_ENV=development API_ENV=prod node script/dev.js",
    "prod": "cross-env NODE_ENV=production API_ENV=prod node script/prod.js",
    "prod:analyze": "cross-env npm run prod && npx webpack-bundle-analyzer dist/assets/analyze.json",
    "release:local": "cross-env NODE_ENV=production LOCAL=true node script/prod.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joe06102/revali-gale.git"
  },
  "keywords": [
    "ssr",
    "react",
    "revali"
  ],
  "author": "xuxiaozhen",
  "license": "ISC",
  "bugs": {
